<html>
  <body>
  <link rel="stylesheet"type="text/css" href="css_file.css">
  <h1 >Music Survey form</h1>
  <br></br>
  <form id="survey-form">
    <div class="row-tab">
    <div class="label-music">
      <label>Name:</lable>
      </div>
      <div class="input-music">
        <input type="text" placeholder="Name">
      </div>
    </div>
      <div class="row-tab">
    <div class="label-music">
      <label>Last Name:</lable>
      </div>
      <div class="input-music">
        <input type="text">
      </div>
    </div>
      <div class="row-tab">
    <div class="label-music">
      <label>Age:</lable>
      </div>
      <div class="input-music">
        <input type="number" placeholder="Age">
      </div>
    </div>
      <div class="row-tab">
    <div class="label-music">
      <label>Email:</lable>
      </div>
      <div class="input-music">
        <input type="email" placeholder="Email">
      </div>
    </div>
     <div class="row-tab">
       <div class="labels-radio">
         Location: 
         </div>
          <div class="input-radio">
            <div>
           <label>India
          <input type="radio" name="location">
          </label>
          </div>
          <div>
          <label> America
          <input type="radio" name="location">
          </label>
          </div>
          <div>
          <label> Africa
          <input type="radio" name="location">
          </label>
          </div>
        </div>
    </div>
    
    <br></br>
    <div class="raw-tab">
      <div class="label-select">
        <label>Select your fav singer: </label>
      </div>
      <div class="select-input">
        <select>
          <option value="Arijit singh">Arijit Singh</option>
           <option value="Lady Gaga">Lady Gaga</option>
           <option value="Taylor Swift">Taylor Swift</option>
           <option value="Shreya Ghosal">Shreya Ghosal</option>
           <option value="Others">Others</option>
        </select>
      </div>
    </div>
   
    
    
    
    
    
  <br></br>
    
    <div class="raw-tab">
      <div class="checkbox-labels">
     
        <label>Select your fav music:  </label>
      </div>
      
      <div class="checkbox-input">
      </div>
      <div>
        <label>
          Bollywood
        <input type="checkbox">
          
        </label>
      </div>
     
     <div>
        <label>
         Folk Music
        <input type="checkbox">
        </label>
      </div>
      <div>
        <label>
          Hip-Hop
        <input type="checkbox">
        </label>
      </div>
      <div>
        <label>
          Others
        <input type="checkbox">
        </label>
      </div>
      </div>
    <div class="row-tab">
      <textarea rows="10" cols="30">Write your comment here....</textarea>
    </div>
    <div class="row-tab">
      <button style="padding:10px" width="400px">Submit
      </button>
      
    </div>
    
  </form>
  </body>
</html>
